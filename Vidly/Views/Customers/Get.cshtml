@model Vidly.ViewModels.CustomerViewModelDetailed

@{
    ViewBag.Title = "Get";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Customer.Name</h2>
<ul>
    <li>Membership Type: @Model.Customer.MembershipType.Name</li>
    @if (@Model.Customer.BirthDate.HasValue)
    {
        <li>Birthdate: @Model.Customer.BirthDate.Value.ToString("yyyy-MM-dd")</li>
    }
</ul>

@Html.Partial("../Movies/ListPartial", Model.Movies)